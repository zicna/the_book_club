<div>
    <%= display_errors(category) %>
    <%= form_for category do |c| %>
       <div>
            <%= c.label :name %>
            <%= c.text_field :name %>
       </div>
        <div>
            <%= c.label :description %>
            <%= c.text_area :description %>
        </div>
        <div><%= c.submit class:"btn btn-success" %></div>
    <% end %>
</div>